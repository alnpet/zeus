<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.alnpet.model" enable-sax-parser="true" enable-base-visitor="true">
	<entity name="model" root="true">
		<entity-ref name="pet" />
	</entity>
	<entity name="pet">
		<attribute name="id" value-type="String" key="true" />
		<attribute name="internal-id" value-type="int" render="false" />
	</entity>
	<entity name="activities">
		<entity-ref name="activity" type="list" names="activities" />
	</entity>
	<entity name="activity">
	    <attribute name="food" value-type="int" primitive="true" />
	    <attribute name="play" value-type="int" primitive="true" />
	    <attribute name="active" value-type="int" primitive="true" />
	    <attribute name="rest" value-type="int" primitive="true" />
	    <attribute name="sport" value-type="int" primitive="true" />
	    <attribute name="sport0" value-type="int" primitive="true" />
	</entity>
	<entity name="item">
	    <attribute name="hour" value-type="int" primitive="true" />
	    <attribute name="food" value-type="int" primitive="true" />
	    <attribute name="play" value-type="int" primitive="true" />
	    <attribute name="active" value-type="int" primitive="true" />
	    <attribute name="rest" value-type="int" primitive="true" />
	</entity>
  <entity name="setting">
    <attribute name="key" value-type="String" key="true"/>
    <element name="text" value-type="String" text="true" alias="value"/>
  </entity>
</model>

