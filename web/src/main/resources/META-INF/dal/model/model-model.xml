<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.alnpet.model" enable-sax-parser="true">
	<entity name="model" root="true">
		<entity-ref name="pet" />
	</entity>
	<entity name="pet">
		<attribute name="id" value-type="String" key="true" />
		<attribute name="internal-id" value-type="int" render="false" />
	</entity>
	<entity name="activities">
		<entity-ref name="activity" type="list" names="activities" />
	</entity>
	<entity name="activity">
		<attribute name="day" value-type="Date" format="yyyy-MM-dd" />
	</entity>
</model>

